{
  "name": "docs-server",
  "version": "1.0.0",
  "description": "文档服务",
  "private": true,
  "dependencies": {
    "decompress": "^4.2.1",
    "egg": "^1.7.0",
    "egg-scripts": "^1.0.0",
    "egg-view-nunjucks": "^2.1.3",
    "fs-extra": "^4.0.1"
  },
  "devDependencies": {
    "@jyb/eslint-config-jyb": "^1.0.1",
    "autod": "^2.9.0",
    "autod-egg": "^1.0.0",
    "egg-bin": "^4.1.0",
    "egg-ci": "^1.8.0",
    "egg-mock": "^3.9.0",
    "eslint": "^4.3.0"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "scripts": {
    "pm2": "pm2 start index.js --name \"doc\"",
    "restart": "pm2 restart doc",
    "start": "NODE_ENV=production node index.js",
    "start:scripts": "egg-scripts start --daemon --port=7004",
    "stop:scripts": "egg-scripts stop",
    "dev": "egg-bin dev",
    "release": "sh release.sh",
    "test": "npm run lint -- --fix && npm run test-local",
    "test-local": "egg-bin test",
    "cov": "egg-bin cov",
    "lint": "eslint .",
    "ci": "npm run lint && npm run cov",
    "autod": "autod"
  },
  "ci": {
    "version": "6, 8"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "canye.wu@jyblife.com",
  "license": "MIT"
}
